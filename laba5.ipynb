    "!pip install web3==4.2.1"
    "from web3 import Web3"
    "from web3 import Web3 \n",
    "web3 = Web3(Web3.HTTPProvider(\"https://mainnet.infura.io/v3/83f8f2d0842247b9a5b4bf0d9927396f\"))\n",
